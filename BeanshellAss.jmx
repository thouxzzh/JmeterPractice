              <stringProp name="script">import java.util.regex.*;
String response=prev.getResponseDataAsString();
Pattern pattern=Pattern.compile(&quot;name=\\\&quot;inputName\\\&quot;\\s+value=\\\&quot;(.*?)\\\&quot;&quot;);
Matcher matcher=pattern.matcher(response);
if (matcher.find()) {
    String firstName=matcher.group(1);
    vars.put(&quot;firstName&quot;,firstName);

    if (response.contains(firstName)) {
        log.info(&quot;First name validated in response:&quot;+firstName);
    } else {
        log.error(&quot;First name not found in response&quot;);
        Failure=true;
    }
} else {
    log.error(&quot;First name not found&quot;);
    Failure=true;
}

            <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor">
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">String[] cities={&quot;Boston&quot;,&quot;New York&quot;,&quot;San Diego&quot;,&quot;Paris&quot;,&quot;Rome&quot;,&quot;London&quot;};
int depIndex=(int)(Math.random()*cities.length);
int destIndex;
do {
    destIndex=(int)(Math.random()*cities.length);
} while (destIndex==depIndex);
vars.put(&quot;fromCity&quot;, cities[depIndex]);
vars.put(&quot;toCity&quot;, cities[destIndex]);
log.info(&quot;From:&quot;+vars.get(&quot;fromCity&quot;)+&quot;To&quot;+vars.get(&quot;toCity&quot;));
</stringProp>
            </BeanShellPreProcessor>

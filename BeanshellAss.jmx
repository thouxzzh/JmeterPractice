              <stringProp name="BeanShellAssertion.query">import java.util.regex.*;
String response=prev.getResponseDataAsString();
Pattern pattern=Pattern.compile(&quot;&lt;p&gt;Total Cost: &lt;em&gt;(.*?) &lt;/em&gt;&lt;/p&gt;&quot;);
Matcher matcher=pattern.matcher(response);
if (matcher.find()) {
    String costStr=matcher.group(1).trim();
    double cost=Double.parseDouble(costStr);
    vars.put(&quot;totalCost&quot;,costStr);
    if (cost&gt;1000) {
        Failure=true;
        FailureMessage=&quot;Total cost exceeds 1000&quot;;
        log.error(FailureMessage);
    }
    else {
        log.info(&quot;Total cost validated&quot;+cost);
    }
} else {
    Failure=true;
    FailureMessage=&quot;Total cost pattern not found in response&quot;;
    log.error(FailureMessage);
}

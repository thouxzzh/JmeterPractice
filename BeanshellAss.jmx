            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion">
              <stringProp name="BeanShellAssertion.query">String response=prev.getResponseDataAsString();
int start=response.indexOf(&quot;Flights from &quot;)+&quot;Flights from &quot;.length();
int end=response.indexOf(&quot;:&quot;,start);
String route=response.substring(start,end).trim();
String destination=vars.get(&quot;toCity&quot;);
if(!route.contains(destination)){
    Failure=true;
    FailureMessage=&quot;Destination city not found&quot;;
    log.error(FailureMessage);
} 
else{
    log.info(&quot;Destination city validated in route&quot;+route);
}

</stringProp>
              <stringProp name="BeanShellAssertion.filename"></stringProp>
